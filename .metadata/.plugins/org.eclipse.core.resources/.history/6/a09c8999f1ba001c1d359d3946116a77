package sample1;

public class Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       byte b[]= {1,2,3,89,78,97};//字节数组
       String str=new String(b);
       System.out.println(str);//输出为乱码
       //字符串转换为字节数组
       String ss="xyz中";
       byte bs[]=ss.getBytes();
       for(byte bt:bs)
       {
    	   System.out.println(bt+"");
       }
	}

}
